{
  "model_name": "physical_attribute_projection",
  "description": "Maps NFL Combine measurables to game attribute ratings",
  "data_source": "NFL Combine data",
  "conversion_formulas": {
    "speed": {
      "source": "forty",
      "formula": "rating = 99 - (forty - 4.22) / (5.60 - 4.22) * 59",
      "description": "Linear scaling: 4.22s = 99, 5.60s = 40",
      "combine_stats": {
        "min": 4.21,
        "max": 5.85,
        "mean": 4.7509744696152465,
        "std": 0.3012337097821492,
        "p10": 4.43,
        "p90": 5.22
      },
      "rating_stats": {
        "min": 40,
        "max": 99,
        "mean": 76.29891760340612,
        "p10": 56.246376811594196,
        "p90": 90.02173913043478
      }
    },
    "agility": {
      "source": "cone",
      "formula": "rating = 99 - (cone - 6.28) / (8.82 - 6.28) * 59",
      "description": "Linear scaling: 6.28s = 99, 8.82s = 40",
      "combine_stats": {
        "min": 6.28,
        "max": 8.82,
        "mean": 7.278953619909502,
        "std": 0.40318171779571593,
        "p10": 6.84,
        "p90": 7.89
      },
      "rating_stats": {
        "min": 40,
        "max": 99,
        "mean": 75.79595922257457,
        "p10": 61.60236220472442,
        "p90": 85.99212598425198
      }
    },
    "strength": {
      "source": "bench",
      "formula": "rating = 40 + (reps - 4) / (49 - 4) * 59",
      "description": "Linear scaling: 4 reps = 40, 49 reps = 99",
      "combine_stats": {
        "min": 4.0,
        "max": 44.0,
        "mean": 19.765174129353234,
        "std": 6.108491596246246,
        "p10": 12.0,
        "p90": 28.0
      },
      "rating_stats": {
        "min": 40,
        "max": 92.44444444444444,
        "mean": 60.66989496959646,
        "p10": 50.48888888888889,
        "p90": 71.46666666666667
      }
    },
    "jumping": {
      "source": "vertical",
      "formula": "rating = 40 + (vertical - 17.5) / (46.5 - 17.5) * 59",
      "description": "Linear scaling: 17.5\" = 40, 46.5\" = 99",
      "combine_stats": {
        "min": 17.5,
        "max": 46.5,
        "mean": 33.07599093997735,
        "std": 4.220798605715335,
        "p10": 27.5,
        "p90": 38.5
      },
      "rating_stats": {
        "min": 40,
        "max": 99,
        "mean": 71.68908501581599,
        "p10": 60.3448275862069,
        "p90": 82.72413793103448
      }
    },
    "acceleration": {
      "source": "forty (derived)",
      "formula": "rating = speed_rating (derived from forty)",
      "description": "Derived from 40-time, correlates with 10-split",
      "note": "No direct measurement; approximated from 40-time"
    },
    "change_of_direction": {
      "source": "shuttle",
      "formula": "rating = 99 - (shuttle - 3.82) / (5.38 - 3.82) * 59",
      "description": "Linear scaling: 3.82s = 99, 5.38s = 40",
      "combine_stats": {
        "min": 3.82,
        "max": 5.38,
        "mean": 4.428478843063739,
        "std": 0.2584305006014277,
        "p10": 4.13,
        "p90": 4.81
      },
      "rating_stats": {
        "min": 40,
        "max": 99,
        "mean": 75.98701811489704,
